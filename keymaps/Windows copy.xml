  </action>
  <action id="QuickDocCopy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
  <action id="XDebugger.CopyValue">
    <keyboard-shortcut first-keystroke="ctrl insert" />
  </action>
</keymap>